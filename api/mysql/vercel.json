{
    "version": 2,
    "name": "gsl-crm-mysql-api",
    "builds": [
        {
            "src": "api/mysql/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/mysql/(.*)",
            "dest": "api/mysql/server.ts"
        },
        {
            "src": "/health",
            "dest": "api/mysql/server.ts"
        },
        {
            "src": "/db-test",
            "dest": "api/mysql/server.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}